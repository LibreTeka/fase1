<p id="notice"><%= notice %></p>

<h1>Listing Libr Libros</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Autor</th>
      <th>Editorial</th>
      <th>Comentario</th>
      <th>Otro1</th>
      <th>Otro2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @libr_libros.each do |libr_libro| %>
      <tr>
        <td><%= libr_libro.titulo %></td>
        <td><%= libr_libro.autor %></td>
        <td><%= libr_libro.editorial %></td>
        <td><%= libr_libro.comentario %></td>
        <td><%= libr_libro.otro1 %></td>
        <td><%= libr_libro.otro2 %></td>
        <td><%= link_to 'Show', libr_libro %></td>
        <td><%= link_to 'Edit', edit_libr_libro_path(libr_libro) %></td>
        <td><%= link_to 'Destroy', libr_libro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Libr libro', new_libr_libro_path %>
